{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/index.js"
      },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "404.html"
    }
    ]
  }
  